	while true do
		wait(0.00000000001)
		if game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed < 50 then
			game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = 100
		end
	end
end)

