local Credits = Credit:NewSection("Credits")

DrinkPoperties:NewButton("Starter Drink Rainbow", "Makes your drink rainbow", function()
	while wait() do
		game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color = Color3.new(1,0,0)
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.r,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.g+(17/255),game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.b)
		end
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.r-(17/255),game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.g,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.b)
		end
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.r,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.g,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.b+(17/255))
		end
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.r,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.g-(17/255),game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.b)
		end
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.r+(17/255),game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.g,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.b)
		end
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.r,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.g,game.Workspace.ienxdfgooi["Starter Drink"].Handle.Color.b-(17/255))
		end
	end
end)

