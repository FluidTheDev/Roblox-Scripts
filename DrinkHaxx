			game:GetService("RunService").RenderStepped:wait()
			game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color.r,game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color.g-(17/255),game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color.b)
