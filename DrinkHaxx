			game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color = Color3.new(game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color.r,game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color.g,game.Workspace.ienxdfgooi["Ninth Drink"].Handle.Color.b+(17/255))
		end
