Teleport:NewToggle("TP to all Hoops", "TP you to all Hoops", function(v)
getgenv().autohooptp = v
while true do
if not getgenv().autohooptp then return end
 wait()
for i, v in pairs(game.Workspace.Hoops:GetChildren()) do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
end
end
end)

Teleport:NewToggle("TP to all coins", "TP you to all coins", function(v)
getgenv().autocointp = v
while true do
if not getgenv().autocointp then return end
wait()
for i, v in pairs(game.Workspace.spawnedCoins.Valley:GetChildren()) do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
end
end
end)

