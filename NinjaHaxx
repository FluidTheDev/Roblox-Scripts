Character:NewButton("WalkSpeed", "Increase your walkspeed", function()
local gmt = getrawmetatable(game)
setreadonly(gmt, false)
local oldindex = gmt.__index
gmt.__index = newcclosure(function(self, b)
    if b == "WalkSpeed" then
        return 16
    end
    return oldindex(self, b)
end)
while wait() do
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=100
end
end)

Character:NewSlider("Inf Double Jump", "Increase your Double Jump", 9999999999, 1, function(v)
game.Players.LocalPlayer.multiJumpCount.Value = v
end)
