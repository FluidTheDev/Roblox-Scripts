local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local NinjaHaxx = Library.CreateLib("NinjaHaxx", "Sentinel")
local Player = NinjaHaxx:NewTab("Player")
local AutoFarm = NinjaHaxx:NewTab("AutoFarm")
local Character = Player:NewSection("Character")
local AutoFarm = AutoFarm:NewSection("AutoFarm")
local Pet = NinjaHaxx:NewTab("Pets")
local Pets = Pet:NewSection("Pets")
local Tp = NinjaHaxx:NewTab("Teleport")
local Teleport = Tp:NewSection("Teleport")
local BossTab = NinjaHaxx:NewTab("Boss")
local Boss = BossTab:NewSection("Boss")

Character:NewToggle("Noclip", "Allow you to walk through walls", function(state)
	local Workspace = game:GetService("Workspace")
	local CoreGui = game:GetService("CoreGui")
	local Players = game:GetService("Players")
	local Plr = Players.LocalPlayer
	local Clipon = false

	if state then
		Clipon = true
		Stepped = game:GetService("RunService").Stepped:Connect(function()
			if not Clipon == false then
				for a, b in pairs(Workspace:GetChildren()) do
					if b.Name == Plr.Name then
						for i, v in pairs(Workspace[Plr.Name]:GetChildren()) do
							if v:IsA("BasePart") then
								v.CanCollide = false
							end end end end
			end
		end)
	else
		Stepped:Disconnect()
	end
end)

AutoFarm:NewToggle("Auto buy Rank ", "Auto buys rank", function(v)
	getgenv().autobuyrank = v
while true do
if not getgenv().autobuyrank then return end
wait()
if game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart") then
local oh1 = "buyRank"
local oh2 = game:GetService("ReplicatedStorage").Ranks.Ground:GetChildren()
for i = 1,#oh2 do
game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(oh1, oh2[i].Name)
end
end
end
end)
Character:NewToggle("Auto Invisible", "Makes your Character Invisible", function(v)
	getgenv().autoinvisible = v
while true do
if not getgenv().autoinvisible then return end
wait()
local A_1 = "goInvisible"
local Event = game.Players.LocalPlayer.ninjaEvent
Event:FireServer(A_1)
end
end)

Boss:NewToggle("Auto Tp to Lavaboss", "Auto TP to the boss", function(v)
	getgenv().autofarmboss = v
while true do
if not getgenv().autofarmboss then return end
wait()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.bossFolder.AncientMagmaBoss.HumanoidRootPart.CFrame
end
end)

