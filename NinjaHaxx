Pets:NewButton("Auto Sell pets", "Auto Sell Pets", function()
	while wait() do
	local A_1 = "sellPet"
	local A_2 = game:GetService("Players").scriptwrit.petsFolder.Unique["Thunder Strike Falcon"]
	local Event = game:GetService("ReplicatedStorage").rEvents.sellPetEvent
	Event:FireServer(A_1, A_2, A_3, A_4)

	local A_1 = "sellPet"
	local A_2 = game:GetService("Players").scriptwrit.petsFolder.Unique["Thunder Strike Falcon"]
	local Event = game:GetService("ReplicatedStorage").rEvents.sellPetEvent
	Event:FireServer(A_1, A_2)
	
	local A_1 = "sellPet"
	local A_2 = game:GetService("Players").scriptwrit.petsFolder.Omega["Eternal Abyss Wyvern"]
	local Event = game:GetService("ReplicatedStorage").rEvents.sellPetEvent
	Event:FireServer(A_1, A_2)
	
	local A_1 = "sellPet"
	local A_2 = game:GetService("Players").scriptwrit.petsFolder.Elite["Dawn Horizon Birdie"]
	local Event = game:GetService("ReplicatedStorage").rEvents.sellPetEvent
	Event:FireServer(A_1, A_2)	
end
end)

Pets:NewToggle("Auto Buy pets", "Auto Buys Pets", function(v)
getgenv().autobuypets = v
while true do
if not getgenv().autobuypets then return end
wait()
local A_1 = "openCrystal"
local A_2 = "Thunder Crystal"
local Event = game:GetService("ReplicatedStorage").rEvents.openCrystalRemote
Event:InvokeServer(A_1, A_2)
end
end)

Pets:NewToggle("Auto Evo pets", "Auto Evo all pets", function(v)
