	Main.Position = UDim2.new(0.304571092, 0, 0.773675799, 0)
	Main.Size = UDim2.new(0, 148, 0, 131)

	CreditsTitle.Name = "Credits-Title"
	CreditsTitle.Parent = Main
	CreditsTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	CreditsTitle.BackgroundTransparency = 1.000
	CreditsTitle.Size = UDim2.new(0, 125, 0, 26)
