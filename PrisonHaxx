-- Scripts:

local function HXCUGDO_fake_script() -- Toggle.LocalScript 
	local script = Instance.new('LocalScript', Toggle)

	local toggled = false

	script.Parent.MouseButton1Down:Connect(function()
		if toggled then
			toggled = false
			script.Parent.Parent.Parent:TweenSize(UDim2.new(0, 605,0, 41), "In", "Linear", 0.1)
			script.Parent.Rotation = 270
		else
			toggled = true
			script.Parent.Parent.Parent:TweenSize(UDim2.new(0, 605,0, 328), "Out", "Linear", 0.1)
			script.Parent.Rotation = 90
		end
	end)
end
coroutine.wrap(HXCUGDO_fake_script)()
