	OwnerCredits.Name = "Owner-Credits"
	OwnerCredits.Parent = Main
	OwnerCredits.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OwnerCredits.BackgroundTransparency = 1.000
	OwnerCredits.Position = UDim2.new(0, 0, 0.396946579, 0)
